---
import { BoardPage } from '@/components/boards/BoardPage';
import DashboardLayout from '@/layouts/DashboardLayout.astro';

const boardTitle = Astro.url.pathname.replace('/dashboard/boards/', '');
---

<DashboardLayout title={boardTitle} containerClassName='p-0'>
	<BoardPage client:only='react' boardName={boardTitle} />
</DashboardLayout>
